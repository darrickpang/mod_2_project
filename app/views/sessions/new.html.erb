<h1>Welcome. Please sign in or sign up.</h1>
<%= form_for :session do |f|  %>
    
    <%= f.label :username %>
    <%= f.text_field :username %> <br/>
    
    <%= f.label :password %>
    <%= f.password_field :password %> <br/>
    
    <button type="submit">Log In</button>
    <%= link_to 'New User', new_user_path %>
  <% end %>
</form>